<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="order_export">
                <settings>
                    <confirm>
                        <message translate="true">Do you want to export selected orders?</message>
                        <title translate="true">Export orders</title>
                    </confirm>
                    <url path="orderexportcustom/order/massExport"/>
                    <type>order_export</type>
                    <label translate="true">Order Export</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
